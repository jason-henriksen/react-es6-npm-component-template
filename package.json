{
  "name": "react-es6-npm-component-template",
  "version": "0.0.2",
  "description": "An empty react component for getting started with an NPM published component",
  "main": "dist/index.js",
  "dependencies": {
    "path": "^0.12.7",
    "react": "^15.5.4",
    "webpack": "^2.6.1"
  },
  "peerDependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.5.1",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-react-app": "^0.6.2",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "jest": "^19.0.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "style-loader": "^0.16.1",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2",
    "prop-types": "^15.5.8"
  },
  "scripts": {
    "server": "./node_modules/.bin/webpack-dev-server --hot --inline",
    "watch": "webpack --watch ",
    "build": "webpack",
    "start": "webpack --watch",
    "test": "jest",
    "test-coverage": "yarn run jest -- --coverage"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "/node_modules/react",
      "/node_modules/react-dom",
      "/node_modules/react-addons-test-utils"
    ],
    "moduleNameMapper": {
      "^.+\\.(css)$": "<rootDir>/preprocessor.js"
    }
  },
